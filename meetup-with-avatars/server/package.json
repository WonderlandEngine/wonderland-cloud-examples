    "dependencies": {
        "@wonderlandcloud/cli": "^0.1.2",
        "@wonderlandcloud/multi-user-server-api": "0.1.1",
        "esbuild": "^0.19.10",
        "typescript": "^5.2.2",
        "yargs": "^17.7.2"
    }
}
